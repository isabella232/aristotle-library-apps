<rdf:RDF
  xmlns:bfabstract="http://bibframe.org/model-abstract/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Person">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Agent"/>
    <dcterms:modified>2013-04-30 (Added example)</dcterms:modified>
    <bfabstract:pipeline-edit>2012-11-30 (New)</bfabstract:pipeline-edit>
    <rdfs:label>Person</rdfs:label>
    <bfabstract:marcField>100,400,600,700,800 I1=0 or 1</bfabstract:marcField>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/15798171"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Annotation">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Resource"/>
    <bfabstract:definition>A resource that adds additional information to or about other BIBFRAME resources when knowing 'who' annotated it is important.</bfabstract:definition>
    <dcterms:modified>2013-04-12 (Updated definition)</dcterms:modified>
    <rdfs:label>Annotation</rdfs:label>
    <bfabstract:marcField>856</bfabstract:marcField>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Organization">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Agent"/>
    <bfabstract:marcField>110,410,610,710,810 I1=0 or 2</bfabstract:marcField>
    <rdfs:label>Organization</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Meeting">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Agent"/>
    <bfabstract:marcField>111,411,611,711,811</bfabstract:marcField>
    <bfabstract:note>RDA corporate conference</bfabstract:note>
    <rdfs:label>Meeting</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Topic">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Authority"/>
    <dcterms:modified>2013-04-30 (Added example)</dcterms:modified>
    <rdfs:label>Topical Concept</rdfs:label>
    <bfabstract:marcField>650</bfabstract:marcField>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/15875548"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Family">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Agent"/>
    <bfabstract:marcField>100,400,600,700,800 I1=2</bfabstract:marcField>
    <rdfs:label>Family</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Place">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Authority"/>
    <dcterms:modified>2013-04-30 (Added example)</dcterms:modified>
    <rdfs:label>Place</rdfs:label>
    <bfabstract:marcField>651</bfabstract:marcField>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/15599751"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Work">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Resource"/>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/17082740"/>
    <bfabstract:definition>A resource reflecting a conceptual essence of the cataloging resource.</bfabstract:definition>
    <rdfs:label>Work</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Instance">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Resource"/>
    <bfabstract:definition>A resource reflecting an individual, material embodiment of the Work.</bfabstract:definition>
    <dcterms:modified>2013-04-17 (Updated)</dcterms:modified>
    <bfabstract:pipeline-edit>2013-04-17 (Updated 856 instance)</bfabstract:pipeline-edit>
    <bfabstract:note>An instance can be generated from an 856 pointing to a digital version of the work.For example, if $ind1=4 and $ind2=1</bfabstract:note>
    <rdfs:label>Instance</rdfs:label>
    <bfabstract:marcField>260</bfabstract:marcField>
    <bfabstract:marcField>856...</bfabstract:marcField>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/11737193"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Jurisdiction">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Agent"/>
    <bfabstract:marcField>110,410,610,710,810 I1=3</bfabstract:marcField>
    <bfabstract:note>RDA corporate jurisdiction</bfabstract:note>
    <rdfs:label>Jurisdiction</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/ClassificationEntity">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Authority"/>
    <dcterms:modified>2013-04-30 (Added example)</dcterms:modified>
    <bfabstract:pipeline-edit>2013-04-10 (New)</bfabstract:pipeline-edit>
    <rdfs:label>Classification Entity</rdfs:label>
    <bfabstract:marcField>050,051,055,060,061,070,071,080,082,083,084,086--/a</bfabstract:marcField>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/15798171"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/TemporalConcept">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Authority"/>
    <dcterms:modified>2013-04-30 (Added example)</dcterms:modified>
    <rdfs:label>Temporal Concept</rdfs:label>
    <bfabstract:marcField>648</bfabstract:marcField>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/15599751"/>
  </rdfs:Class>
  <rdf:Description rdf:about="http://bibframe.org/vocab/hasAnnotation">
    <rdfs:label>Has Annotation</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://bibframe.org/vocab/descriptionRole">
    <bfabstract:marcField>If 100,110,111 then = "primary"</bfabstract:marcField>
    <rdfs:label>Role with respect to the description.</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://bibframe.org/vocab/identifier">
    <rdfs:label>identifier</rdfs:label>
  </rdf:Description>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Resource">
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/authorizedAccessPoint">
        <rdfs:label>Authorized access point</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/label">
        <rdfs:label>Label for resource</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/relatedResource">
        <bfabstract:marcField>730,740, and i2 not 2</bfabstract:marcField>
        <bfabstract:marcField>700,710,711, with $t and i2 not 2</bfabstract:marcField>
        <bfabstract:marcField>787</bfabstract:marcField>
        <rdfs:label>Related resource</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/description">
        <rdfs:label>Description of resource</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range rdf:resource="http://bibframe.org/vocab/identifier"/>
    <rdfs:label>Resource</rdfs:label>
  </rdfs:Class>
  <rdf:Description rdf:about="http://bibframe.org/vocab/system-number">
    <bfabstract:marcField>$0</bfabstract:marcField>
    <bfabstract:marcField>035--/a,z</bfabstract:marcField>
    <rdfs:label>system control number</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://bibframe.org/vocab/viaf">
    <bfabstract:marcField>authority:0247-+2"via,zf"/a,z</bfabstract:marcField>
    <rdfs:label>Virtual International Authority File number</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://bibframe.org/vocab/isni">
    <bfabstract:marcField>authority:0247-+2"isni"/a,z</bfabstract:marcField>
    <rdfs:label>International Standard Name Identifier</rdfs:label>
  </rdf:Description>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Agent">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Authority"/>
    <rdfs:range rdf:resource="http://bibframe.org/vocab/descriptionRole"/>
    <rdfs:range rdf:resource="http://bibframe.org/vocab/viaf"/>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/resourceRole">
        <bfabstract:marcField>if 111,411,611,711,811--j,4</bfabstract:marcField>
        <bfabstract:marcField>100,400,600,700,800, 110,410,610,710,810--/e,4</bfabstract:marcField>
        <rdfs:label>Role with respect to resource.</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range rdf:resource="http://bibframe.org/vocab/isni"/>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/orcid">
        <bfabstract:marcField>authority:0247-+2"orcid"/a,z</bfabstract:marcField>
        <rdfs:label>Open Researcher and Contributor Identifier</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <dcterms:modified>2013-05-09 (Added example)</dcterms:modified>
    <rdfs:label>Agent</rdfs:label>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/17159572"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Authority">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Resource"/>
    <bfabstract:definition>A resource reflecting key authority concepts that have defined relationships reflected in the Work and Instance.</bfabstract:definition>
    <rdfs:label>Authority</rdfs:label>
    <rdfs:range rdf:resource="http://bibframe.org/vocab/hasAnnotation"/>
    <rdfs:range rdf:resource="http://bibframe.org/vocab/system-number"/>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/hasAuthority">
        <bfabstract:marcField>$0, 035</bfabstract:marcField>
        <rdfs:label>Has Authority</rdfs:label>
      </rdf:Description>
    </rdfs:range>
  </rdfs:Class>
</rdf:RDF>
