<?xml version="1.0" encoding="utf-8"?><!--
            The BIBFRAME Vocabulary is a draft and under development.  It is subject to change.
                -->
<rdf:RDF
  xmlns:bfabstract="http://bibframe.org/model-abstract/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Authority">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Resource"/>
    <bfabstract:definition>A resource reflecting key authority concepts that have defined relationships reflected in the Work and Instance.</bfabstract:definition>
    <rdfs:label>Authority</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Instance">
    <bfabstract:note>An instance can be generated from an 856 pointing to a digital version of the work.For example, if $ind1=4 and $ind2=1</bfabstract:note>
    <bfabstract:marcField>856...</bfabstract:marcField>
    <bfabstract:marcField>260</bfabstract:marcField>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/11737193"/>
    <bfabstract:pipeline-edit>2013-04-17 (Updated 856 instance)</bfabstract:pipeline-edit>
    <bfabstract:definition>A resource reflecting an individual, material embodiment of the Work.</bfabstract:definition>
    <rdfs:label>Instance</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Resource"/>
    <dcterms:modified>2013-04-17 (Updated)</dcterms:modified>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/CoverArt">
    <bfabstract:marcField>856</bfabstract:marcField>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/16574557"/>
    <rdfs:label>Cover Art Annotation</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Annotation"/>
    <dcterms:modified>2013-04-18 (Updated MARC); 2013-04-24 (Added example)</dcterms:modified>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Work">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Resource"/>
    <bfabstract:definition>A resource reflecting a conceptual essence of the cataloging resource.</bfabstract:definition>
    <rdfs:label>Work</rdfs:label>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/17082740"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Review">
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Annotation"/>
    <bfabstract:marcField>856</bfabstract:marcField>
    <rdfs:label>Review Annotation</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Holding">
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/callno-udc">
        <rdfs:label>UDC call number for a resource</rdfs:label>
        <bfabstract:marcField>080--/a+b+c</bfabstract:marcField>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/callno">
        <rdfs:label>Call number for a resource</rdfs:label>
        <bfabstract:marcField>084--/a+b</bfabstract:marcField>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/callno-ddc">
        <rdfs:label>DDC call number for a resource</rdfs:label>
        <bfabstract:marcField>082--/a+b</bfabstract:marcField>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/callno-lcc">
        <rdfs:label>LCC call number for a resource</rdfs:label>
        <bfabstract:marcField>050,051,055,060,061,070,071--/a+b</bfabstract:marcField>
      </rdf:Description>
    </rdfs:range>
    <bfabstract:marcField>852</bfabstract:marcField>
    <bfabstract:marcExample rdf:resource="http://id.loc.gov/resources/bibs/17082740"/>
    <bfabstract:pipeline-edit>2013-04-18 (New)</bfabstract:pipeline-edit>
    <rdfs:label>Holding</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Annotation"/>
    <dcterms:modified>2013-04-16</dcterms:modified>
  </rdfs:Class>
  <rdf:Description rdf:about="http://bibframe.org/vocab/description">
    <rdfs:label>Description of resource</rdfs:label>
  </rdf:Description>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Annotation">
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/annotates">
        <rdfs:label>Target of Annotation</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/annotationBody">
        <rdfs:label>Annotation itself</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/annotationAssertedBy">
        <rdfs:label>Annotation asserted by</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <bfabstract:marcField>856</bfabstract:marcField>
    <dcterms:modified>2013-04-12 (Updated definition)</dcterms:modified>
    <rdfs:label>Annotation</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://bibframe.org/vocab/Resource"/>
    <bfabstract:definition>A resource that adds additional information to or about other BIBFRAME resources when knowing 'who' annotated it is important.</bfabstract:definition>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://bibframe.org/vocab/Resource">
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/relatedResource">
        <rdfs:label>Related resource</rdfs:label>
        <bfabstract:marcField>787</bfabstract:marcField>
        <bfabstract:marcField>700,710,711, with $t and i2 not 2</bfabstract:marcField>
        <bfabstract:marcField>730,740, and i2 not 2</bfabstract:marcField>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/authorizedAccessPoint">
        <rdfs:label>Authorized access point</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/label">
        <rdfs:label>Label for resource</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <rdfs:range rdf:resource="http://bibframe.org/vocab/description"/>
    <rdfs:range>
      <rdf:Description rdf:about="http://bibframe.org/vocab/identifier">
        <rdfs:label>identifier</rdfs:label>
      </rdf:Description>
    </rdfs:range>
    <rdfs:label>Resource</rdfs:label>
  </rdfs:Class>
</rdf:RDF>
